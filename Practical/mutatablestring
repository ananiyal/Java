package Ananiyal;

public class mutable {
	private String a;

	public mutable(String a) {
		this.a=a;
	}

	public String getA() {
		return a;
	}

	public void setA(String a) {
		this.a = a;
	}

	public static void main(String[] args) {
		mutable b=new mutable("Ananiyal");
		System.out.println(b.getA());
		b.setA("nairobi");
		System.out.println(b.getA());

	}

}
